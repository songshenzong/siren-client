{
  "name": "songshenzong/siren-client",
  "description": "Siren Client",
  "keywords": [
    "Siren",
    "UDP",
    "TCP",
    "Http",
    "Log"
  ],
  "license": "MIT",
  "homepage": "http://songshenzong.com",
  "support": {
    "issues": "https://github.com/songshenzong/siren-client/issues",
    "source": "https://github.com/songshenzong/siren-client"
  },
  "authors": [
    {
      "name": "Songshenzong",
      "email": "i@songshenzong.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "ramsey/uuid": "^3.7",
    "songshenzong/http-client": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "laravel/framework": "5.6.*"
  },
  "autoload": {
    "psr-4": {
      "Songshenzong\\SirenClient\\": "src/",
      "Songshenzong\\SirenClient\\Test\\": "tests/"
    },
    "files": [
      "src/Helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Songshenzong\\SirenClient\\ServiceProvider"
      ],
      "aliases": {
        "SirenClient": "Songshenzong\\SirenClient\\Facade"
      }
    }
  }
}
